<!DOCTYPE html>
<html lang="en">

<%- include('include/_head') %>

    <body>
        <% if(typeof error !== undefined) { %>

            <h1>Oops! Something went wrong. </h1>
            <div class="errorMsg">
                <p>
                    <%= errors %>
                </p>
                <br>
                <a href="/">Click here to try again</a>
            </div>
            <% } %>
    </body>

</html>